{
    "name": "alekseyshavrak/laravel-smsru",
    "description": "SmsRu Notifications Driver",
    "homepage": "https://github.com/alekseyshavrak/laravel-smsru",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Aleksey Shavrak",
            "email": "alekseyshavrak@gmail.com",
            "homepage": "https://github.com/alekseyshavrak"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "~5.7|~5.8|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SmsRu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\SmsRu\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
